msgid "Wind"
msgstr "Eolico"

#: templates/tool.html:418
msgid "Biomass"
msgstr "Biomasse"

#: templates/tool.html:420
msgid "Coal"
msgstr "Carbone"

#: templates/tool.html:422
msgid "Oil"
msgstr "Petrolio"

#: templates/tool.html:424
msgid "Geothermal"
msgstr "Geotermia"

#: templates/tool.html:426
msgid "Waste"
msgstr "Rifiuti"

#: templates/tool.html:433
msgid "Set to zero"
msgstr "Impostare su zero"

#: templates/tool.html:440
msgid "Adjust the number of passengers"
msgstr "Modifica il numero di passeggeri"

#: templates/tool.html:450
msgid "Adjust the cargo mass"
msgstr "Modifica la massa della merce"

